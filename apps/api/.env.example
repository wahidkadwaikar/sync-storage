NODE_ENV=development
PORT=4000

# Production defaults to AUTH_MODE=jwt. Use AUTH_MODE=none only for local/dev.
AUTH_MODE=none
JWT_SECRET=replace-me

STORAGE_DRIVER=sqlite
SQLITE_FILE_PATH=./data/sync-storage.sqlite
# TURSO_URL=libsql://...
# TURSO_AUTH_TOKEN=
# POSTGRES_URL=postgresql://...
# REDIS_URL=redis://localhost:6379
# REDIS_PASSWORD=
# REDIS_DB=0

DEFAULT_NAMESPACE=default
DEFAULT_TENANT_ID=default
MAX_KEY_LENGTH=255
MAX_VALUE_BYTES=1048576
MAX_BATCH_SIZE=100
MAX_LIST_LIMIT=100
